<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Indent Sequences -->
        <record id="sequence_mrp_indent" model="ir.sequence">
            <field name="name">Indent</field>
            <field eval="5" name="padding"/>
            <field name="prefix">Indent/%(year)s/</field>
            <field name="code">indent.order</field>
        </record>
        <!-- Stock Indent Sequences -->
        <record id="sequence_mrp_indent_stock" model="ir.sequence">
            <field name="name">Indent</field>
            <field eval="5" name="padding"/>
            <field name="prefix">INV-IND-%(year)s/</field>
            <field name="code">stock.indent.order</field>
        </record>
        <record model="res.groups" id="group_material">
            <field name="name">Approve Material Requisition</field>
        </record>
    </data>
</odoo>