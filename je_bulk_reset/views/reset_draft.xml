<odoo>
  <data>
    <!-- action for reset to draft -->
      <record id="ir_action_resetdraft_reset_draft" model="ir.actions.server">
        <field name="name">Reset to draft</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="account.model_account_move"/>
        <field name="state">code</field>
        <field name="code">
            if records:
            action = records.button_resetdraft()
        </field>
        <field name="binding_model_id" ref="account.model_account_move"/>
      </record>

    <!-- action to set record to remove entry -->
      <record id="ir_action_resetdraft_delete_entry" model="ir.actions.server">
        <field name="name">Set to Delete</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="account.model_account_move"/>
        <field name="state">code</field>
        <field name="code">
            if records:
            action = records.button_removeentry()
        </field>
        <field name="binding_model_id" ref="account.model_account_move"/>
      </record>


  </data>
</odoo>